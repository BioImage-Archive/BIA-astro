---
import BaseLayoutWithBreadcrumbs from "../../../../layouts/BaseLayoutWithBreadcrumbs.astro";
import ModelPage from "../../../../components/ai-benchmarking/ModelPage.astro"

import modelMetadata from "../../../../data/ai-dataset-benchmarking/model-metadata.json"
import tableInfo from "../../../../data/ai-dataset-benchmarking/model-dataset-table.json"

export function getStaticPaths() {
  return Object.keys(modelMetadata).map((id) => {
    return {
      params: { modelName: id },
    };
  });
}

const { modelName } = Astro.params;


const breadcrumbs = [
  "/bioimage-archive",
  "/bioimage-archive/galleries",
  "/bioimage-archive/galleries/ai",
  "/bioimage-archive/galleries/ai/models",
]
---
<BaseLayoutWithBreadcrumbs pageTitle={modelName} breadcrumbs={breadcrumbs}>
  <ModelPage modelName={modelName} modelMetadata={modelMetadata} tableInfo={tableInfo} aiProjectLogosBasepath="/bioimage-archive/ai-galleries/"/>
</BaseLayoutWithBreadcrumbs>