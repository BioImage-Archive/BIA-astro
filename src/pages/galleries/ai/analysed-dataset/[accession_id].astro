---
import BaseLayout from "../../../../layouts/BaseLayout.astro";

import BenchmarkedDatasetPage from "../../../../components/ai-benchmarking/BenchmarkedDatasetPage.astro"

import CollectionList from "../../../../data/collections/ai-benchmark-datasets.json"
import StudyMetadata from "../../../../data/bia-study-metadata.json"
import AdditionalStudyMetadata from "../../../../data/ai-dataset-benchmarking/additional-study-metadata.json"
import TableInfo from "../../../../data/ai-dataset-benchmarking/model-dataset-table.json"


export function getStaticPaths() {
  return Object.values(CollectionList["accession_ids"]).map((id) => {
    return {
      params: { accession_id: id },
    };
  });
}

const { accession_id } = Astro.params;

---

<BaseLayout pageTitle={accession_id}>
  <BenchmarkedDatasetPage accession_id={accession_id} StudyMetadata={StudyMetadata} AdditionalStudyMetadata={AdditionalStudyMetadata} TableInfo={TableInfo}/>
</BaseLayout>


