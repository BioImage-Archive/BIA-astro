const pageData = {
    help: "Help Overview",
    faq: "FAQ",      
    helpsearch: "Searching Data",
    helpdownload:"Downloading Data",
    helpimagesatebi: "Image Data at EBI",
    helptools: "Supporting Tools"
}
const submHelp = {
  helpfilelist: "File List Help",
    submitannotations: "Submit Annotations",
    aiglossary: "Annotation Types"
}
const rembiHelp = {
  rembihelpoverview: "REMBI Overview",
  rembihelplab: "REMBI Lab Guide",
  rembihelpexamples: "REMBI Examples",
