---
//const pagenames = ["Home", "Browse", "Submit", "Galleries", "Help", "Policies", "About"];
const pageData = {
    "": "Home",
    datasets: "Browse",
    submit: "Submit",
    vis: "Galleries",
    help: "Help",
    policies: "Policies",
    about: "About"
}
---
<nav class="vf-navigation vf-navigation--main | vf-cluster" style="margin-top: 0.5rem;">
    <ul class="vf-navigation__list | vf-list | vf-cluster__inner">
      {Object.entries(pageData).map(([key, value]) => (
        <li class="vf-navigation__item">
            <a href={`/${key}`} class="vf-navigation__link">{value}</a>
        </li>
        )
      )}
    </ul>
</nav>